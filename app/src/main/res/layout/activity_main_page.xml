<?xml version="1.0" encoding="utf-8"?>
 <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainPage"
    android:background="@color/white">


    <TextView
        android:id="@+id/greetingUser"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="200dp"
        android:fontFamily="@font/inter"
        android:gravity="center_vertical"
        android:letterSpacing="-0.05"
        android:lineHeight="30sp"
        android:lineSpacingExtra="10sp"
        android:text="Welcome back!"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/settingBtn"
        android:layout_width="47dp"
        android:layout_height="47dp"
        android:layout_marginStart="282dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="31dp"
        android:background="@drawable/welcomepage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/setting" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="330dp"
        android:layout_height="300dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle" />

    <TextView
        android:id="@+id/description"
        android:layout_width="270dp"
        android:layout_height="27.44dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="55dp"
        android:layout_marginTop="140dp"
        android:layout_marginEnd="48dp"
        android:fontFamily="@font/inter"
        android:gravity="top"
        android:letterSpacing="-0.02"
        android:lineHeight="28sp"
        android:lineSpacingExtra="6sp"
        android:text="Percentage of latest test taken"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:translationY="3.11sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_mainPage"
        android:layout_width="193dp"
        android:layout_height="36dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="83dp"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="83dp"
        android:fontFamily="@font/inter"
        android:gravity="center_horizontal"
        android:letterSpacing="-0.03"
        android:lineHeight="18sp"
        android:lineSpacingExtra="3sp"
        android:text="Breath Alcohol Concentration levels on 23%"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:translationY="1.74sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/setupButton"
        android:layout_width="165dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="450dp"
        android:layout_marginEnd="200dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="#DBFF00"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/contactButton"
        android:layout_width="165dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="200dp"
        android:layout_marginTop="450dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="#00E0FF"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/testButton"
        android:layout_width="165dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="540dp"
        android:layout_marginEnd="200dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="#F23557"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/locationButton"
        android:layout_width="165dp"
        android:layout_height="75dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="200dp"
        android:layout_marginTop="540dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="#00FF47"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/settingText"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/inter"
        android:gravity="left"
        android:letterSpacing="-0.08"
        android:lineHeight="20sp"
        android:lineSpacingExtra="0sp"
        android:text="Set up your device"
        android:textColor="@color/black"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="@+id/setupButton"
        app:layout_constraintEnd_toEndOf="@+id/setupButton"
        app:layout_constraintStart_toStartOf="@+id/setupButton"
        app:layout_constraintTop_toTopOf="@+id/setupButton" />

    <TextView
        android:id="@+id/contactText"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/inter"
        android:gravity="left"
        android:letterSpacing="-0.08"
        android:lineHeight="20sp"
        android:lineSpacingExtra="0sp"
        android:text="Emergency contacts"
        android:textColor="@color/black"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="@+id/contactButton"
        app:layout_constraintEnd_toEndOf="@+id/contactButton"
        app:layout_constraintStart_toStartOf="@+id/contactButton"
        app:layout_constraintTop_toTopOf="@+id/contactButton" />

    <TextView
        android:id="@+id/testText"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/inter"
        android:gravity="center_vertical"
        android:letterSpacing="-0.08"
        android:lineHeight="20sp"
        android:lineSpacingExtra="0sp"
        android:text="Test Record"
        android:textColor="@color/black"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="@+id/testButton"
        app:layout_constraintEnd_toEndOf="@+id/testButton"
        app:layout_constraintStart_toStartOf="@+id/testButton"
        app:layout_constraintTop_toTopOf="@+id/testButton" />

    <TextView
        android:id="@+id/locationText"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/inter"
        android:gravity="center_vertical"
        android:letterSpacing="-0.08"
        android:lineHeight="20sp"
        android:lineSpacingExtra="0sp"
        android:text="Share live location"
        android:textColor="@color/black"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="@+id/locationButton"
        app:layout_constraintEnd_toEndOf="@+id/locationButton"
        app:layout_constraintStart_toStartOf="@+id/locationButton"
        app:layout_constraintTop_toTopOf="@+id/locationButton" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/setupButton"
        app:layout_constraintEnd_toEndOf="@+id/setupButton"
        app:layout_constraintStart_toStartOf="@+id/setupButton"
        app:layout_constraintTop_toTopOf="@+id/setupButton"
        app:srcCompat="@drawable/tool" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/contactButton"
        app:layout_constraintEnd_toEndOf="@+id/contactButton"
        app:layout_constraintStart_toStartOf="@+id/contactButton"
        app:layout_constraintTop_toTopOf="@+id/contactButton"
        app:srcCompat="@drawable/user" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/testButton"
        app:layout_constraintEnd_toEndOf="@+id/testButton"
        app:layout_constraintStart_toStartOf="@+id/testButton"
        app:layout_constraintTop_toTopOf="@+id/testButton"
        app:srcCompat="@drawable/clipboard" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/locationButton"
        app:layout_constraintEnd_toEndOf="@+id/locationButton"
        app:layout_constraintStart_toStartOf="@+id/locationButton"
        app:layout_constraintTop_toTopOf="@+id/locationButton"
        app:srcCompat="@drawable/maplogo"/>

   <ProgressBar
       android:id="@+id/progressBar"
       android:layout_width="200dp"
       android:layout_height="200dp"
       android:layout_marginStart="105dp"
       android:layout_marginTop="170dp"
       android:layout_marginEnd="105dp"
       android:indeterminateOnly="false"
       android:progressDrawable="@drawable/custom_progress"
       android:progress="23"
       android:rotation="-90"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toTopOf="parent" />

   <TextView
       android:id="@+id/percentageProgressBar"
       android:layout_width="90dp"
       android:layout_height="52dp"
       android:layout_marginStart="150dp"
       android:layout_marginTop="240dp"
       android:layout_marginEnd="150dp"
       android:fontFamily="@font/inter"
       android:gravity="center_horizontal|center"
       android:letterSpacing="-0.03"
       android:lineHeight="52sp"
       android:lineSpacingExtra="1sp"
       android:text="23%"
       android:textColor="@color/white"
       android:textSize="42sp"
       android:translationY="-0.59sp"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintHorizontal_bias="0.508"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>